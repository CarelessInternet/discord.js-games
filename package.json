{
	"name": "discord.js-games",
	"version": "1.1.8",
	"description": "A discord.js v13 module for minigames",
	"main": "./dist/src/index.js",
	"types": "./dist/src/index.d.ts",
	"scripts": {
		"start": "node ./dist/discord-testing/index.js",
		"deploy": "node ./dist/discord-testing/deploy.js",
		"test": "node ./dist/discord-testing/test.js",
		"build": "tsc",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint . --ext .ts",
		"prepublishOnly": "npm run format && npm run lint && npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CarelessInternet/discord.js-games.git"
	},
	"keywords": [
		"discord.js",
		"games",
		"minigames",
		"nodejs",
		"fun"
	],
	"author": "CarelessInternet",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/CarelessInternet/discord.js-games/issues"
	},
	"homepage": "https://github.com/CarelessInternet/discord.js-games#readme",
	"devDependencies": {
		"@discordjs/rest": "^0.5.0",
		"@types/node": "^17.0.42",
		"@typescript-eslint/eslint-plugin": "^5.35.1",
		"@typescript-eslint/parser": "^5.27.1",
		"dotenv": "^16.0.1",
		"eslint": "^8.17.0",
		"node-fetch": "^2.6.7",
		"prettier": "^2.6.2",
		"typescript": "^4.7.3"
	},
	"dependencies": {
		"@discordjs/builders": "^0.15.0",
		"discord.js": "^13.8.0"
	},
	"engines": {
		"node": ">=16.6.0"
	},
	"files": [
		"dist/src/**/*"
	]
}