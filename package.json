{
	"name": "discord.js-games",
	"version": "1.0.2",
	"description": "A Discord.js v13 module for minigames",
	"main": "./dist/src/index.js",
	"types": "./dist/src/index.d.ts",
	"scripts": {
		"start": "node ./dist/discord-testing/index.js",
		"deploy": "node ./dist/discord-testing/deploy.js",
		"test": "node ./dist/discord-testing/test.js",
		"build": "tsc",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint . --ext .ts",
		"prepublishOnly": "npm run format && npm run lint && npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CarelessInternet/discord.js-games.git"
	},
	"keywords": [
		"discord.js",
		"games",
		"minigames",
		"nodejs",
		"fun"
	],
	"author": "CarelessInternet",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/CarelessInternet/discord.js-games/issues"
	},
	"homepage": "https://github.com/CarelessInternet/discord.js-games#readme",
	"devDependencies": {
		"@discordjs/rest": "^0.1.0-canary.0",
		"@types/node": "^16.11.7",
		"@types/node-fetch": "^2.5.12",
		"@types/shuffle-array": "^1.0.2",
		"@typescript-eslint/eslint-plugin": "^5.4.0",
		"@typescript-eslint/parser": "^5.4.0",
		"dotenv": "^10.0.0",
		"eslint": "^8.2.0",
		"path": "^0.12.7",
		"prettier": "^2.4.1",
		"typescript": "^4.4.4"
	},
	"dependencies": {
		"@discordjs/builders": "^0.8.2",
		"discord.js": "^13.3.1"
	},
	"engines": {
		"node": ">=16.6.0"
	},
	"files": [
		"dist/src/**/*"
	]
}
